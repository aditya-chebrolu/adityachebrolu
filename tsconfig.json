{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "baseUrl": "./src",
    "paths": {
      "@components/*":["components/*"],
      "@styles/*":["styles/*"],
      "@layouts/*":["layouts/*"],
      "@hooks/*":["hooks/*"],
      "@icons/*":["icons/*"],
      "@assets/*":["assets/*"],
      "@modules/*":["modules/*"],
      "@constants/*":["constants/*"],
    }
  }
}